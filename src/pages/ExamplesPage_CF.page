                    .appendTo(ul);
        }

        /*
         * Example for input file to upload files using remoting.
         *
         * @param lstFiles  List of files to be uploaded.
         */
        function myfunctionToApplyRemoting(lstFiles) {
            boolEvalFunctionInputFileResult = true;
            var strFiles = "";
            for(strIndex in lstFiles) {
                var arrRules = strIndex.split('&&&');
                strFiles = strFiles.concat(arrRules[intFileNameIndexPosition]+ ", ").toString();
            }
            alert('Executing Base 64 Input File for: ' + strFiles);
        }

