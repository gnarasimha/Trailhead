         *
         * @param strQuery Query.
         * @param response Response
         */
        function searchExample(strQuery, response) {
            var objList = [];
            ExamplesController_CF.remoteAction(strQuery, function(result, event) {
                if(event.type != 'exception') {
                    for(var intCounter = 0; intCounter < result.length; intCounter++){
                        var objRecord = new Object();
                        objRecord.strName = result[intCounter].strName;
                        objRecord.strPosition = result[intCounter].strPosition;
                        objRecord.strOffice = result[intCounter].strOffice;
                        objRecord.strSalary = result[intCounter].strSalary;
                        objList.push(objRecord);
                    }
                    response(objList);
                }
            });
        }

        /*
         * Select example function.
         */
        function selectExample(event, ui) {
            alert('On selection: ' + ui.item.strName);
            $('#htmlField').val(ui.item.strName);
            return false;
        }

        /*
         * Result example function.
         */
        function resultExample(ul, item) {
            return $("<li>")
